doctype html
html
    head
        block head
            meta(charset="UTF-8")
            meta(name="viewport", content="width=device-width, initial-scale=1.0")
            link(rel="preconnect", href="https://fonts.googleapis.com")
            link(rel="preconnect", href="https://fonts.gstatic.com", crossorigin)
            link(href="https://fonts.googleapis.com/css2?family=Roboto+Mono:wght@200;400;600&display=swap", rel="stylesheet")
            script(src="https://kit.fontawesome.com/09446dd6c3.js", crossorigin="anonymous")
            link(rel="stylesheet", href="../dist/css/style.min.css")
            title Project: Global Forces #{title}

    block class
    body(class=bodyClass)
        include _nav

        block content

        include _footer

        script(src="./dist/js/main.min.js" defer)
        block scripts